<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#3C3F41"
    tools:context=".Start11">

    <Button
        android:id="@+id/h1"
        android:layout_width="186dp"
        android:layout_height="65dp"
        android:layout_marginTop="119dp"
        android:fontFamily="@font/delius"
        android:text="HOME"
        android:textAlignment="center"
        android:textSize="30sp"
        app:backgroundTint="#445945"
        app:cornerRadius="10sp"
        app:iconTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yvsc" />

    <Button
        android:id="@+id/yvsc"
        android:layout_width="282dp"
        android:layout_height="74dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="65dp"
        android:fontFamily="@font/delius"
        android:text="YOU VS CREATOR"
        android:textAlignment="center"
        android:textSize="25sp"
        app:backgroundTint="#445945"
        app:cornerRadius="10sp"
        app:iconTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/twop" />

    <Button
        android:id="@+id/twop"
        android:layout_width="282dp"
        android:layout_height="74dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="127dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="68dp"
        android:fontFamily="@font/delius"
        android:text="2 PLAYER"
        android:textAlignment="center"
        android:textSize="30sp"
        app:backgroundTint="#445945"
        app:cornerRadius="10sp"
        app:iconTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/yvsc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>